{
  "compilerOptions": {
    "target": "es2022",
    "skipLibCheck": true,
    "noEmit": true,
    "lib": ["es2022", "dom"],
    "module": "commonjs",
    "strict": true,
    "moduleResolution": "nodenext",
    "paths": {
      "jsforce": ["../"],
      "jsforce/*": ["../*"]
    },
    "typeRoots": ["typings", "node_modules/@types"],
    "esModuleInterop": true
  },
  "include": ["./**/*.ts"]
}
