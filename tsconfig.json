{
  "compilerOptions": {
    "target": "es2022",
    "skipLibCheck": true,
    "lib": ["es2022", "dom"],
    "module": "commonjs",
    "strict": true,
    "moduleResolution": "nodenext",
    "paths": {
      "jsforce": ["."],
      "jsforce/*": ["./*"],
    },
    "typeRoots": [
      "typings",
      "node_modules/@types"
    ],
    "esModuleInterop": true 
  }
}
